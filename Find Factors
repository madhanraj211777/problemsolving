package second;
import java.util.*;

public class factors {

	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		int num;
		System.out.println("enter the num");
		num=scan.nextInt();
		for(int i=1;i<=num;i++) {
			if(num%i==0) {
				System.out.println("the factor of"+num+"is "+i );
			}
		}
		

	}

}
