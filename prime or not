package second;
import java.util.*;

public class prime {

	public static void main(String[] args) {
		Scanner scan =new Scanner(System.in);
		int count=0;
		System.out.println("enter the number:");
		int num=scan.nextInt();
		for(int i=1;i<=num;i++) {
			if(num%i==0) {
				count=count+1;
			}
		}
		if(count==2) {
			System.out.println("it is prime");
		}
		else {
			System.out.println("it is not a prime");
		}

	}

}

