package second;
import java.util.*;
public class factorial {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		 int num;
		Scanner scan=new Scanner(System.in);
		System.out.println("enter the number:");
		num=scan.nextInt();
		int fact=1;
		for(int i=1;i<=num;i++) {
			fact=fact*i;
			
		}
		System.out.println(fact);

	}

}
