import java.util.*;
public class Main
{
	public static void main(String[] args) {
		Scanner scan=new Scanner(System.in);
		System.out.println("enter the number:");
		int n=scan.nextInt();
		System.out.println("enter the kth bit:");
		int k=scan.nextInt();
		int temp=1;
		temp=(n&(1<<(k-1)));
		if(temp==1){
		    System.out.println("  set");
		}
		else{
		    System.out.println(" not  set");
		}
	}
}
