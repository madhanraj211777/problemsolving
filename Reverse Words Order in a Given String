import java.util.*;
import java.lang.*;
class HelloWorld {
    public static void main(String[] args) {
        Scanner scan=new Scanner(System.in);
      String s ;
      System.out.println("enter the string");
      s=scan.nextLine();
      String r="";
      
      for(int i=s.length()-1;i>=0;i--){
          r=r+(s.charAt(i));
          
      }
    
         System.out.print(r);
    
       
       
    }
}
