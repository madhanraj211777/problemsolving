
public class Main
{
	public static void main(String[] args) {
		int a[]={6,4,8,3,1};
		for(int i=1;i<a.length;i++){
		    int temp=a[i];
		    int j=i-1;
		    while(j>=0&&a[j]>temp){
		        a[j+1]=a[j];
		        j=j-1;
		    }
		    a[j+1]=temp;
		}
		for(int num:a){
		    System.out.print(num+" ");
		}
	}
}
